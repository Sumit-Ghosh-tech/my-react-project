{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "app/categories/page.jsx", "app/cart/page.jsx", "app/about/page.jsx", "app/login/page.jsx", "app/contact/page.jsx", "app/product/[id]/Productdetails.tsx", "components/footer.jsx", "components/header.jsx", "components/search-modal.jsx", "components/theme-provider.jsx", "app/checkout/page.jsx", "app/signup/page.jsx", "app/shop/page.jsx", "app/layout.jsx", "app/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
